<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.idsbg.foxconn.myapplication"
    android:versionCode="1"
    android:versionName="1.0">



    <application
        android:allowBackup="true"
        android:icon="@drawable/tubiao"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity android:name=".okhttp.MainActivity">
            <intent-filter>
                <!--                指定该Activity是程序的入口-->
                <action android:name="android.intent.action.MAIN" />
                <!--                指定加载该应用时运行该activity-->
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DongHua"
            android:label="播放动画"></activity>
        <activity
            android:name=".ExpandableListActivityTest"
            android:label="查看动物种类"></activity>
        <activity
            android:name=".Base.Register"
            android:label="注册界面"></activity>
        <activity
            android:name=".ResultActivity"
            android:exported="true"
            android:label="注册结果显示"></activity>

        <provider
            android:name=".DictProvider"
            android:authorities="com.idsbg.foxconn.myapplication.dictprovider"
            android:exported="true" />
        <service android:name=".BindService" />
    </application>
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <!--    在sd卡中创建、删除文件权限-->
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />
    <!--    向sd写入数据的权限-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--    授权访问互联网-->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--    获取本应用读取短信的权限-->
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
</manifest>

